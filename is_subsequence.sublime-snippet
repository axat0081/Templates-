bool is_subsequence(string s,string t)
{
	int n=s.length(),m=t.length(); 
	int j=0; 
	rep(i,0,n-1)
	{
     if(s[i]==t[j]){ j++; }
     if(j==m){ return true; }
	}
	return false;
}
]]></content>
