	<content><![CDATA[
long long strNum(string &s)
{
	long long ans=0; 
	for(int i=0;i<=(int)s.size()-1;i++)
	{
	  int d=s[i]-48; 
	  ans=(ans*10)+d; 
	}
	return ans; 
}
]]></content>
